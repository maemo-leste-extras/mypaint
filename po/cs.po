#: ../gui/brushselectionwindow.py:36
#: ../gui/drawwindow.py:106
msgid "Edit"
msgstr "Upravit"

#: ../gui/brushselectionwindow.py:152
msgid "Right click on group to modify"
msgstr "Skupinu upravíte kliknutím pravým tlačítkem"

#: ../gui/brushselectionwindow.py:234
msgid "New group..."
